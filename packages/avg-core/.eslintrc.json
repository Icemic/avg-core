{
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": "react-app"
}